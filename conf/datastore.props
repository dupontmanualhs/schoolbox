datanucleus.ConnectionDriverName=org.h2.Driver
datanucleus.ConnectionURL=jdbc:h2:data;AUTO_SERVER=true;MVCC=true

datanucleus.autoCreateSchema=true
datanucleus.DetachAllOnCommit=true
datanucleus.maxFetchDepth=1

datanucleus.autoStartMechanism=Classes
datanucleus.autoStartClassNames=models.users.Role,models.courses.Student,models.courses.Teacher,models.courses.Guardian,models.users.Permission

datanucleus.plugin.allowUserBundles=true
datanucleus.plugin.validatePlugins=true
